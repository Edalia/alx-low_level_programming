#include "main.h"
/**
*more_numbers - print 0 - 14
*
*Return: void
*/

void more_numbers(void)
{
	int x = 0;

	while (x < 10)
	{
		int i = 0;

		while (i < 15)
		{
			if (i < 10)
			      	_putchar('0' + i);
			   
			else
			{
				_putchar('0' + (i / 10));
				_putchar('0' + (i % 10));
			}
			i++;
		}
		_putchar('\n');
		x++;
	}
}
